{
  "host_id": "",
  "host_name": "IIS Administration API",
  "security": {
    "windows_authentication": true,
    "users": {
      "administrators": [
      ],
      "owners": [
      ]
    },
    "access_policy": {
      "api": "users:administrators+AccessKey",
      "api_keys": "users:administrators",
      "system": "users:owners+AccessKey"
    }
  },
  "logging": {
    "enabled": true,
    "min_level": "error",
    "file_name": "log-{Date}.txt"
  },
  "auditing": {
    "enabled": true,
    "file_name": "audit-{Date}.txt"
  },
  "cors": {
    "rules": [
      {
        "origin": "https://manage.iis.net",
        "allow": true
      }
    ]
  }
}